            		;;
        		esac
	;;
    	"r666b")
        	case $monitor_frequency in
            		"15KHz")
                 		crt_range="15450-16050, 50-65, 3.190, 4.750, 6.450, 0.191, 0.191, 1.164, 0, 0, 192, 288, 448, 576"
            		;;
            		"24KHz")
                 		crt_range="23900-24900, 50-65, 2.870, 3.000, 4.440, 0.451, 0.164, 1.148, 0, 0, 384, 400, 0, 0"
            		;;
            		"31KHz")
                 		crt_range="31000-32500, 50-65, 0.330, 3.580, 1.750, 0.316, 0.063, 1.137, 0, 0, 400, 512, 0, 0"
            		;;
            		*)
            		;;
        	esac       
    	;;
    	"pc_31_120")
        	case $monitor_frequency in
            		"31KHz")           
                		case $monitor_frame in
                    			"120Hz")
                         			crt_range="31400-31600, 100-130, 0.671, 2.683, 3.353, 0.034, 0.101, 0.436, 0, 0, 200, 256, 0, 0"
                    			;;
                    			"60Hz")
                         			crt_range="31400-31600, 50-65, 0.671, 2.683, 3.353, 0.034, 0.101, 0.436, 0, 0, 400, 512, 0, 0"
                    			;;
                    			*)
                    			;;
                		esac
            		;;
            		*)
            		;;
        	esac
    	;;
    	"pc_70_120")
        	case $monitor_frequency in
            		"70KHz")           
                		case $moniotr_frame in
                    			"120Hz")   
                         			crt_range="30000-70000, 100-130, 2.201, 0.275, 4.678, 0.063, 0.032, 0.633, 0, 0, 192, 320, 0, 0"
                    			;;
	    	     			"60Hz")
                         			crt_range="30000-70000, 50-65, 2.201, 0.275, 4.678, 0.063, 0.032, 0.633, 0, 0, 400, 1024, 0, 0"
                    			;;
                    			*)
                    			;;
                		esac
            		;;
            		*)
            		;;
