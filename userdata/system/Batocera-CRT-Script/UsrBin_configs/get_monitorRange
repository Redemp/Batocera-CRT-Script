			;;
			*)
		esac
    	;;
    	"pstar")
        	case $monitor_frequency in
            		"15KHz")
                 		crt_range="15700-15800, 50-65, 1.800, 0.400, 7.400, 0.064, 0.160, 1.056, 0, 0, 192, 256, 0, 0"
            		;;
            		"16KHz")
