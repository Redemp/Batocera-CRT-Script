#!/bin/bash

get_monitorRange(){

    local monitor_name=$1
    local monitor_frequency=$2
    local monitor_frame=$3
    local crt_range  

    case $monitor_name in

 	"pal") 
	    	case $monitor_frequency in
                	"15KHz")
            	    		crt_range="15625.00-15625.00, 50.00-50.00, 1.500, 4.700, 5.800, 0.064, 0.160, 1.056, 0, 0, 192, 288, 448, 576"
		    	;;
	    		*)
			;;
    		esac    
	;;
        "ntsc")
		case $monitor_frequency in
                	"15KHz")
                		crt_range="15734.26-15734.26, 59.94-59.94, 1.500, 4.700, 4.700, 0.191, 0.191, 0.953, 0, 0, 192, 240, 448, 480"
			;;
	    		*)
			;;
    		esac    
	;;
        "generic_15")
		case $monitor_frequency in
